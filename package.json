{
  "name": "redux-github-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register test/**/*.js*",
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:js",
    "watch": "npm run watch:html & npm run watch:js",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp public/index.html build/",
    "watch:html": "npm run build:html && chokidar index.html -c 'npm run build:html'",
    "clean:html": "rm -f build/index.html",
    "build:js": "npm run clean:js && webpack",
    "watch:js": "npm run build:js && webpack --watch",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dobs1211/redux-github-app.git"
  },
  "author": "Olga Chavez ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dobs1211/redux-github-app/issues"
  },
  "homepage": "https://github.com/dobs1211/redux-github-app#readme",
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.1.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
